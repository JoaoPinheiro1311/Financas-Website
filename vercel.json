{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/callback/(.*)",
      "destination": "/api"
    },
    {
      "source": "/login/(.*)",
      "destination": "/api"
    },
    {
      "source": "/ws/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
